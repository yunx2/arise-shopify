<script>
  // store specific config variables for vue app
  const shopConfigs = { // what else lives in here/will be different from store to store
    shopId: 1,
    oAuthCallbackUrl: 'https://cymbiotika.com/pages/arise',
    portalUrl: 'https://9hbmvnwm20.execute-api.us-west-1.amazonaws.com'
  }

  const imageSources = {% render 'images-transparent.js' %}
  const formatImageSources = dataset => {
    const split = dataset.split('\n'); // ex ['Plant Protein,https://cdn.shopify.com/s/files/1/1824/8017/files/26May22_Quiz_PlantProtein.png?v=1653589839,...]
    const formatted = split.map(str => {
      const arr = str.split(',');
      return arr;
    });
    const imageDict = {};
    formatted.forEach(tuple => {
      const [title, id, src] = tuple;
      imageDict[id] = src;
    });
    return imageDict; // ex [['Plant Protein', 'https://cdn.shopify.com/s/files/1/1824/8017/files/26May22_Quiz_PlantProtein.png?v=1653589839'], ...]
  }


let products = []
{%- assign collection = collections['cymbiotika_bundle_recharge'] -%}
{% for product in collection.products %}
   {%- assign lower = product.title | downcase -%}
 {% if product.tags contains 'merch' or lower contains 'preorder' or product.available == false  %}
    {% continue %}
{%endif%}
products.push({
    tags: {{ product.tags | json }},
    url: "{{ product.url }}",
    handle: "{{ product.handle }}",
    subtitle: "{{ product.metafields.descriptors.subtitle }}",
    price: parseInt("{{ product.price }}") / 100,
    productId: parseInt("{{ product.id }}"),
    variantId: parseInt("{{ product.variants[0].id }}"),
    title: `{{ product.title }}`,
  })
{% endfor %}
const imageDict = formatImageSources(imageSources);

products.forEach(p => p.image = imageDict[p.productId]);

  //console.log(‘products’, products)
  let orders = []
  {% for order in customer.orders %}
    orders.push({
    created_at: "{{ order.created_at }}",
    line_items: {{ order.line_items | json }}
    })
  {% endfor %}
  if (orders.length > 2) {
    orders =  orders.slice(0, 2)
  }
  orders.forEach(el => {
    const lineItems = el.line_items.map(item => {
      return {
        productId: item.product.id,
        title: item.title
      }
    })
    el.line_items = lineItems
  })
 // const order_count = {{ customer.orders_count }}
  // console.log("orders", orders)
 // console.log(“order_count”, order_count)
</script>
